    "@astrojs/vercel": "7.1.1",
    "@clerk/clerk-sdk-node": "4.13.8",
    "@neondatabase/serverless": "0.8.1",
    "@t3-oss/env-core": "0.9.2",
    "@vercel/speed-insights": "1.0.9",
    "alpinejs": "3.13.5",
    "astro": "4.3.3",
    "drizzle-orm": "0.29.3",
    "htmx.org": "1.9.10",
    "minimatch": "9.0.3",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3",
