{
  "jsonRecursiveSort": true,
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro"
      }
    }
  ],
  "plugins": [
    "prettier-plugin-astro",
    "prettier-plugin-organize-imports",
    "prettier-plugin-sort-json",
    "prettier-plugin-tailwindcss"
  ],
  "printWidth": 120,
  "singleQuote": true
}
